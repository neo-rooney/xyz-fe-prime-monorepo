{
  "name": "@xyz/react-components-layout",
  "packageManager": "yarn@4.3.1",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "yarn build:js --watch && yarn build:type --watch",
    "build": "yarn clean && yarn build:js && yarn build:type",
    "build:js": "node build.js",
    "build:type": "yarn tsc --emitDeclarationOnly",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vanilla-extract/css": "^1.15.3",
    "@vanilla-extract/esbuild-plugin": "^2.3.8",
    "@xyz/esbuild": "workspace:^",
    "@xyz/themes": "workspace:^",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "@xyz/themes": "workspace:^",
    "react": "*"
  }
}
